<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/calenderbutton"
        android:layout_width="93dp"
        android:layout_height="98dp"
        android:layout_marginTop="156dp"
        android:layout_marginEnd="268dp"
        android:background="@drawable/ic_baseline_calendar_month_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/stopwatchbutton"
        android:layout_width="91dp"
        android:layout_height="95dp"
        android:layout_marginTop="156dp"
        android:layout_marginEnd="76dp"
        android:background="@drawable/ic_baseline_access_alarms_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bmibutton"
        android:layout_width="93dp"
        android:layout_height="80dp"
        android:layout_marginTop="296dp"
        android:layout_marginEnd="268dp"
        android:background="@drawable/ic_baseline_accessibility_new_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/todobutton"
        android:layout_width="86dp"
        android:layout_height="67dp"
        android:layout_marginTop="296dp"
        android:layout_marginEnd="92dp"
        android:background="@drawable/ic_baseline_format_list_bulleted_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/foodbutton"
        android:layout_width="98dp"
        android:layout_height="104dp"
        android:layout_marginTop="396dp"
        android:layout_marginEnd="156dp"
        android:background="@drawable/ic_baseline_flatware_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>